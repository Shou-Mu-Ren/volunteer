{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// 引入组件 以及 插件\nimport Header from \"@/components/Header.vue\";\nimport Footer from \"@/components/Footer.vue\";\nimport Pagination from \"@/components/pagenation.vue\";\nimport Swiper from \"swiper\";\nimport \"swiper/css/swiper.min.css\";\nexport default {\n  name: \"Home\",\n  components: {\n    Header,\n    Footer,\n    Pagination\n  },\n  data() {\n    return {\n      swiperList: [],\n      schoolType: \"本科\",\n      hotSchool: {},\n      schoolList: [],\n      hotMajor: {},\n      marjotList: [],\n      areaList: [],\n      volunteerList: [],\n      subjectList: [{\n        title: \"物理\",\n        active: false\n      }, {\n        title: \"化学\",\n        active: false\n      }, {\n        title: \"生物\",\n        active: false\n      }, {\n        title: \"政治\",\n        active: false\n      }, {\n        title: \"历史\",\n        active: false\n      }, {\n        title: \"地理\",\n        active: false\n      }],\n      maskFlag: true,\n      messageFlag: true,\n      total: 20,\n      pagination: {\n        pageSize: 9,\n        p: 1\n      },\n      url: \"http://47.93.21.24:3001/api\",\n      token: \"\",\n      member: {},\n      subjectNum: 0,\n      message: {\n        year: \"\",\n        grand: \"\",\n        rank: \"\",\n        subject: \"\"\n      }\n    };\n  },\n  mounted() {\n    this.token = window.localStorage.getItem(\"X-token\");\n    this.getMemberInfo();\n    this.getSchoolList();\n    this.getMajorList();\n    this.getAreaList();\n    this.getBannerList();\n    this.getNesList();\n    this.$nextTick(() => {\n      new Swiper(\".bannerSwiper\", {\n        loop: true,\n        autoplay: {\n          delay: 5000 //1秒切换一次\n        },\n\n        pagination: {\n          el: \".swiper-pagination\",\n          clickable: true\n        },\n        navigation: {\n          nextEl: \".swiper_right\",\n          prevEl: \".swiper_left\"\n        },\n        observer: true,\n        observeParents: true\n      });\n    });\n  },\n  methods: {\n    // 获取个人信息\n    getMemberInfo() {\n      let _this = this;\n      this.$http.get(this.url + \"/user/info\", {\n        headers: {\n          Authorization: _this.token\n        }\n      }).then(function (res) {\n        if (res.data.code == 200) {\n          _this.member = res.data.data;\n          if (res.data.data.grand != null) {\n            _this.maskFlag = false;\n            _this.messageFlag = false;\n          }\n        }\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    // 获取轮播图\n    getBannerList() {\n      let _this = this;\n      this.$http.post(this.url + \"/img/list\", {}, {\n        headers: {\n          Authorization: _this.token\n        }\n      }).then(function (res) {\n        if (res.data.code == 200) {\n          _this.swiperList = res.data.data;\n        }\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    // 获取学校数据\n    getSchoolList() {\n      let _this = this;\n      this.$http.post(this.url + \"/university/heat\", {\n        level: _this.schoolType\n      }, {\n        headers: {\n          Authorization: _this.token\n        }\n      }).then(function (res) {\n        if (res.data.code == 200) {\n          _this.schoolList = res.data.data;\n          _this.hotSchool = res.data.data[0];\n        }\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    // 切换为本科\n    changeBenke() {\n      this.schoolType = \"本科\";\n      this.getSchoolList();\n    },\n    // 切换为专科\n    changeZhuanke() {\n      this.schoolType = \"专科\";\n      this.getSchoolList();\n    },\n    // 获取专业数据\n    getMajorList() {\n      let _this = this;\n      this.$http.post(this.url + \"/major/heat\", {}, {\n        headers: {\n          Authorization: _this.token\n        }\n      }).then(function (res) {\n        if (res.data.code == 200) {\n          _this.marjotList = res.data.data;\n          _this.hotMajor = res.data.data[0];\n        }\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    // 获取热门地区\n    getAreaList() {\n      let _this = this;\n      this.$http.post(this.url + \"/city/heat\", {}, {\n        headers: {\n          Authorization: _this.token\n        }\n      }).then(function (res) {\n        if (res.data.code == 200) {\n          _this.areaList = res.data.data;\n        }\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    // 获取资讯\n    getNesList() {\n      let _this = this;\n      this.$http.post(this.url + \"/article/list\", {}, {\n        headers: {\n          Authorization: _this.token\n        }\n      }).then(function (res) {\n        if (res.data.code == 200) {\n          _this.volunteerList = res.data.data;\n        }\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n    changePage(pager) {\n      this.pagination.p = pager.page;\n      // 分页器回调\n      this.$emit(\"change\", pager);\n    },\n    // 选择高考科目\n    changeSubject(item) {\n      if (item.active == false && this.subjectNum < 3) {\n        this.subjectNum++;\n        item.active = true;\n      } else if (item.active == true) {\n        this.subjectNum--;\n        item.active = false;\n      }\n    },\n    // 填写高考信息\n    anylyse() {\n      if (this.message.year == \"\") {\n        this.$message({\n          message: \"请填写高考年份\",\n          type: \"warning\"\n        });\n      } else if (this.message.grand == \"\") {\n        this.$message({\n          message: \"请填写高考总分\",\n          type: \"warning\"\n        });\n      } else if (this.message.grand == \"\") {\n        this.$message({\n          message: \"请填写全省排名\",\n          type: \"warning\"\n        });\n      } else {\n        let subjects = [];\n        for (var i = 0; i < this.subjectList.length; i++) {\n          if (this.subjectList[i].active == true) {\n            subjects.push(this.subjectList[i].title);\n          }\n        }\n        this.message.subject = subjects.join(\"/\");\n        if (this.message.subject == \"\") {\n          this.$message({\n            message: \"请选择高考科目\",\n            type: \"warning\"\n          });\n        } else if (this.subjectNum != 3) {\n          this.$message({\n            message: \"请选择3门高考科目\",\n            type: \"warning\"\n          });\n        } else {\n          let _this = this;\n          this.$http.post(this.url + \"/user/analyse\", _this.message, {\n            headers: {\n              Authorization: _this.token\n            }\n          }).then(function (res) {\n            if (res.data.code == 200) {\n              _this.$message({\n                message: \"消息填写成功\",\n                type: \"warning\"\n              });\n              _this.getMemberInfo();\n            }\n          }).catch(function (error) {\n            console.log(error);\n          });\n        }\n      }\n    },\n    toFill() {\n      this.$router.push({\n        name: \"fill\"\n      });\n    },\n    tofindshool() {\n      this.$router.push({\n        name: \"findschool\"\n      });\n    },\n    tofindMajor() {\n      this.$router.push({\n        name: \"findmajor\"\n      });\n    },\n    toVip() {\n      this.$router.push({\n        name: \"vip\"\n      });\n    },\n    toMember() {\n      this.$router.push({\n        name: \"info\"\n      });\n    },\n    toTop() {\n      document.documentElement.scrollTop = 0;\n    }\n  }\n};","map":{"version":3,"names":["Header","Footer","Pagination","Swiper","name","components","data","swiperList","schoolType","hotSchool","schoolList","hotMajor","marjotList","areaList","volunteerList","subjectList","title","active","maskFlag","messageFlag","total","pagination","pageSize","p","url","token","member","subjectNum","message","year","grand","rank","subject","mounted","window","localStorage","getItem","getMemberInfo","getSchoolList","getMajorList","getAreaList","getBannerList","getNesList","$nextTick","loop","autoplay","delay","el","clickable","navigation","nextEl","prevEl","observer","observeParents","methods","_this","$http","get","headers","Authorization","then","res","code","catch","error","console","log","post","level","changeBenke","changeZhuanke","changePage","pager","page","$emit","changeSubject","item","anylyse","$message","type","subjects","i","length","push","join","toFill","$router","tofindshool","tofindMajor","toVip","toMember","toTop","document","documentElement","scrollTop"],"sources":["src/pages/home/index.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 头部组件 -->\n    <Header></Header>\n\n    <!-- 轮播图 -->\n    <section class=\"banner_section\">\n      <div class=\"swiper-container bannerSwiper\">\n        <div class=\"swiper-wrapper\">\n          <div\n            class=\"swiper-slide\"\n            v-for=\"(item, index) in swiperList\"\n            :key=\"index\"\n          >\n            <img :src=\"item.img\" alt=\"\" />\n          </div>\n        </div>\n        <div class=\"swiper-pagination\"></div>\n        <div class=\"swiper_right\">\n          <img src=\"../../assets/uploads/right.png\" alt=\"\" />\n        </div>\n        <div class=\"swiper_left\">\n          <img src=\"../../assets/uploads/left.png\" alt=\"\" />\n        </div>\n      </div>\n    </section>\n\n    <!-- 分类区域 -->\n    <section class=\"type_section\">\n      <div class=\"container\">\n        <div class=\"types\">\n          <div class=\"type\">\n            <span class=\"iconfont icon-xuexiao\"></span>\n            <em>热门学校</em>\n          </div>\n          <div class=\"type\">\n            <span class=\"iconfont icon-renshizhuanye\"></span>\n            <em>热门专业</em>\n          </div>\n          <div class=\"type\">\n            <span class=\"iconfont icon-diqu\"></span>\n            <em>热门地区</em>\n          </div>\n          <div class=\"type\">\n            <span class=\"iconfont icon-zixun\"></span>\n            <em>高考咨询</em>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 学校区域 -->\n    <section class=\"school_section\">\n      <div class=\"container\">\n        <div class=\"school\">\n          <div class=\"school_hot\">\n            <div class=\"school_hot_title\">\n              <div class=\"school_hot_title_line\"></div>\n              热门学校\n            </div>\n            <div class=\"school_hot_content\">\n              <div class=\"school_hot_content_title\">\n                {{ hotSchool.name }}\n              </div>\n              <div class=\"school_hot_content_tags\">\n                <div class=\"school_hot_content_tag\">\n                  {{ hotSchool.type }}\n                </div>\n              </div>\n              <div class=\"school_hot_content_img\">\n                <img :src=\"hotSchool.img\" alt=\"\" />\n              </div>\n            </div>\n          </div>\n          <div class=\"school_list\">\n            <div class=\"school_nav\">\n              <div\n                class=\"school_nav_item\"\n                :class=\"schoolType == '本科' ? 'active' : ''\"\n                @click=\"changeBenke()\"\n              >\n                本科\n              </div>\n              <div\n                class=\"school_nav_item\"\n                :class=\"schoolType == '专科' ? 'active' : ''\"\n                @click=\"changeZhuanke()\"\n              >\n                专科\n              </div>\n            </div>\n            <div class=\"school_ul\">\n              <div\n                class=\"school_li\"\n                v-for=\"(item, index) in schoolList\"\n                :key=\"index\"\n              >\n                <div class=\"school_li_img\">\n                  <img :src=\"item.img\" alt=\"\" />\n                </div>\n                <div class=\"school_li_msg\">\n                  <div class=\"school_li_title\">\n                    {{ item.name }}\n                  </div>\n                  <div class=\"school_li_other\">\n                    <div class=\"school_li_tag\">\n                      {{ item.type }}\n                    </div>\n                    <div class=\"school_li_more\">查看院校 &gt;</div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 专业区域 -->\n    <section class=\"school_section\">\n      <div class=\"container\">\n        <div class=\"school\">\n          <div class=\"school_hot\">\n            <div class=\"school_hot_title\">\n              <div class=\"school_hot_title_line\"></div>\n              热门专业\n            </div>\n            <div class=\"school_hot_content\">\n              <div class=\"school_hot_content_title\">\n                {{ hotMajor.name }}\n              </div>\n              <div class=\"school_hot_content_tags\">\n                <div class=\"school_hot_content_tag\">\n                  {{ hotMajor.type }}\n                </div>\n              </div>\n              <div class=\"school_hot_content_img\">\n                <img :src=\"hotMajor.img\" alt=\"\" />\n              </div>\n            </div>\n          </div>\n          <div class=\"school_list major_list\">\n            <div class=\"school_ul\">\n              <div\n                class=\"school_li\"\n                v-for=\"(item, index) in marjotList\"\n                :key=\"index\"\n              >\n                <div class=\"school_li_img\">\n                  <img :src=\"item.img\" alt=\"\" />\n                </div>\n                <div class=\"school_li_msg\">\n                  <div class=\"school_li_title\">\n                    {{ item.name }}\n                  </div>\n                  <div class=\"school_li_other\">\n                    <div class=\"school_li_tag\">\n                      {{ item.type }}\n                    </div>\n                    <div class=\"school_li_more\">查看院校 &gt;</div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 热门地区 -->\n    <section class=\"area_section\">\n      <div class=\"container\">\n        <div class=\"area\">\n          <div class=\"area_title\">\n            <div class=\"area_title_line\"></div>\n            热门地区\n          </div>\n          <div class=\"area_list\">\n            <div\n              class=\"area_item\"\n              v-for=\"(item, index) in areaList\"\n              :key=\"index\"\n            >\n              <img :src=\"item.img\" alt=\"\" />\n              <p>\n                {{ item.name }}\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 高考资讯区域 -->\n    <section class=\"volunteer_section\">\n      <div class=\"container\">\n        <div class=\"volunteer_title\">\n          <div class=\"volunteer_line\"></div>\n          高考资讯\n        </div>\n        <div class=\"volunteer\">\n          <div class=\"volunteer_lf\">\n            <div class=\"volunteer_lf_img\">\n              <img src=\"../../assets/uploads/home/zhiyuan.jpg\" alt=\"\" />\n            </div>\n            <div class=\"volunteer_lf_location\">山东</div>\n            <div class=\"volunteer_lf_li\">志愿填报</div>\n            <div class=\"volunteer_lf_li\">高校资讯</div>\n          </div>\n          <div class=\"volunteer_rt\">\n            <div\n              class=\"volunteer_item\"\n              v-for=\"(item, index) in volunteerList\"\n              :key=\"index\"\n            >\n              <div class=\"volunteer_item_msg\">\n                <div class=\"volunteer_item_title\">\n                  {{ item.content }}\n                </div>\n                <div class=\"volunteer_item_other\">\n                  {{ item.author }} · {{ item.createTime }} ·\n                  {{ item.lookCount }}次阅读\n                </div>\n              </div>\n              <div class=\"volunteer_item_img\">\n                <img :src=\"item.img\" alt=\"\" />\n              </div>\n            </div>\n            <!-- <Pagination\n              @change=\"changePage\"\n              :current=\"pagination.p\"\n              :pageSize=\"pagination.pageSize\"\n              :total=\"total\"\n              :showQuickJumper=\"true\"\n              :showTotal=\"true\"\n              placement=\"right\"\n              v-if=\"pagination && total\"\n            /> -->\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <div class=\"mask\" v-if=\"maskFlag == true\"></div>\n\n    <!-- 高考信息区域 -->\n    <div class=\"message\" v-if=\"messageFlag == true\">\n      <div class=\"message_hd\">\n        <div class=\"message_title\">高考信息填写</div>\n        <div class=\"message_tips\">\n          请填写真实高考信息，注意点击“开始分析”后，不能修改\n        </div>\n      </div>\n      <div class=\"message_bd\">\n        <div class=\"message_item\">\n          <div class=\"message_label\">高考省份:</div>\n          <div class=\"message_input\">\n            <input\n              type=\"text\"\n              placeholder=\"请填写高考省份\"\n              value=\"山东省\"\n              disabled=\"false\"\n            />\n          </div>\n        </div>\n        <div class=\"message_item\">\n          <div class=\"message_label\">高考年份:</div>\n          <div class=\"message_input\">\n            <input\n              type=\"text\"\n              placeholder=\"请填写高考年份\"\n              v-model=\"message.year\"\n            />\n          </div>\n        </div>\n        <div class=\"message_item\">\n          <div class=\"message_label\">高考总分:</div>\n          <div class=\"message_input\">\n            <input\n              type=\"text\"\n              placeholder=\"请填写高考总分\"\n              v-model=\"message.grand\"\n            />\n          </div>\n        </div>\n        <div class=\"message_item\">\n          <div class=\"message_label\">全省排名:</div>\n          <div class=\"message_input\">\n            <input\n              type=\"text\"\n              placeholder=\"请填写全省排名\"\n              v-model=\"message.rank\"\n            />\n          </div>\n        </div>\n        <div class=\"message_subjects\">\n          <div class=\"message_label\">高考选科:</div>\n          <div class=\"subjects\">\n            <div\n              class=\"message_subject\"\n              v-for=\"(item, index) in subjectList\"\n              :key=\"index\"\n              :class=\"item.active == true ? 'active' : ''\"\n              @click=\"changeSubject(item)\"\n            >\n              {{ item.title }}\n            </div>\n          </div>\n        </div>\n        <div class=\"message_btn\" @click=\"anylyse\">\n          <div>开始分析</div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 侧边栏区域 -->\n    <aside class=\"aside_section\">\n      <div class=\"aside\" @click=\"toFill\">\n        <span class=\"iconfont icon-tianbao\"></span>\n      </div>\n      <div class=\"aside\" @click=\"tofindshool\">\n        <span class=\"iconfont icon-xuexiao\"></span>\n      </div>\n      <div class=\"aside\" @click=\"tofindMajor\">\n        <span class=\"iconfont icon-renshizhuanye\"></span>\n      </div>\n      <div class=\"aside\" @click=\"toVip\">\n        <span class=\"iconfont icon-VIP\"></span>\n      </div>\n      <div class=\"aside\" @click=\"toMember\">\n        <span class=\"iconfont icon-gerenzhongxin\"></span>\n      </div>\n      <div class=\"aside\" @click=\"toTop\">\n        <span class=\"iconfont icon-zhiding\"></span>\n      </div>\n    </aside>\n\n    <!-- 尾部组件 -->\n    <Footer></Footer>\n  </div>\n</template>\n\n<script>\n// 引入组件 以及 插件\nimport Header from \"@/components/Header.vue\";\nimport Footer from \"@/components/Footer.vue\";\nimport Pagination from \"@/components/pagenation.vue\";\n\nimport Swiper from \"swiper\";\nimport \"swiper/css/swiper.min.css\";\n\nexport default {\n  name: \"Home\",\n  components: {\n    Header,\n    Footer,\n    Pagination,\n  },\n  data() {\n    return {\n      swiperList: [],\n\n      schoolType: \"本科\",\n      hotSc