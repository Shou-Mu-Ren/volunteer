s.data.data;\n            if (res.data.data.grand != null) {\n              _this.maskFlag = false;\n              _this.messageFlag = false;\n            }\n          }\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n\n    // 获取轮播图\n    getBannerList() {\n      let _this = this;\n      this.$http\n        .post(\n          this.url + \"/img/list\",\n          {},\n          {\n            headers: {\n              Authorization: _this.token,\n            },\n          }\n        )\n        .then(function (res) {\n          if (res.data.code == 200) {\n            _this.swiperList = res.data.data;\n          }\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n\n    // 获取学校数据\n    getSchoolList() {\n      let _this = this;\n      this.$http\n        .post(\n          this.url + \"/university/heat\",\n          {\n            level: _this.schoolType,\n          },\n          {\n            headers: {\n              Authorization: _this.token,\n            },\n          }\n        )\n        .then(function (res) {\n          if (res.data.code == 200) {\n            _this.schoolList = res.data.data;\n            _this.hotSchool = res.data.data[0];\n          }\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n    // 切换为本科\n    changeBenke() {\n      this.schoolType = \"本科\";\n      this.getSchoolList();\n    },\n    // 切换为专科\n    changeZhuanke() {\n      this.schoolType = \"专科\";\n      this.getSchoolList();\n    },\n\n    // 获取专业数据\n    getMajorList() {\n      let _this = this;\n      this.$http\n        .post(\n          this.url + \"/major/heat\",\n          {},\n          {\n            headers: {\n              Authorization: _this.token,\n            },\n          }\n        )\n        .then(function (res) {\n          if (res.data.code == 200) {\n            _this.marjotList = res.data.data;\n            _this.hot